ul
    each val in data
        li
            -if (val['MOD_PCON_END'] == null){
            -   var yearEnd = '.';
            -}
            -else{
            -   var yearEnd = String(val['MOD_PCON_END']).substring(4,6) +'.'+ String(val['MOD_PCON_END']).substring(0, 4);
            -}
            -var dataProd = '(' + String(val['MOD_PCON_START']).substring(4,6) +'.'+ String(val['MOD_PCON_START']).substring(0, 4) + ' - '+ yearEnd + ')';
            a(href='#', class='form-link-model', data-mod_id = val['mod_id'])= val['MOD_CDS_TEXT'] + ' ' + dataProd